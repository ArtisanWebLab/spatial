{
    "name": "artisanweblab/spatial",
    "description": "MySQL and PostgreSQL spatial data types extension for Laravel",
    "license": "MIT",
    "authors": [
        {
            "name": "Oleksandr Shapoval",
            "email": "shapoval.webdev@gmail.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "ext-json": "*",
        "ext-pdo": "*",
        "artisanweblab/geojson": "dev-master",
        "laravel/framework": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "ArtisanWebLab\\Spatial\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "ArtisanWebLab\\Spatial\\SpatialDatabaseServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
